{
    "env": {
        "browser": false,
        "node": true
    },
    "globals": {
        "Promise": true
    },
    "rules": {
        "eol-last": 2,
        "eqeqeq": 2,
        "indent": [2, 2],
        "key-spacing": [2],
        "max-len": [2, 120, 2],
        "no-irregular-whitespace": 2,
        "no-loop-func": 2,
        "no-multiple-empty-lines": [1, {"max": 2}],
        "no-shadow": [1],
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"],
        "no-unused-vars": 1,
        "quotes": [2, "single"],
        "semi": 2,
        "space-infix-ops": [2],
        "space-before-blocks": [2, "always"],
        "space-after-keywords": [2, "always"],
        "strict": [2, "never"]
    }

}